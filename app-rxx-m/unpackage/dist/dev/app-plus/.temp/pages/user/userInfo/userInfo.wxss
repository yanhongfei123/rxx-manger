@import "../../../common/vendor.wxss";

@charset "UTF-8";
.flex {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}
.box-list {
  font-size: 30rpx;
  padding: 20rpx 30rpx;
}
.box-list .box-label {
  width: 250rpx;
}
.box-list .box-cont {
  overflow: hidden;
}
.box-list .box-item {
  float: left;
  width: 40%;
  margin: 0 0 20rpx;
  padding: 0;
}
.select-btn {
  width: 24rpx;
  height: 24rpx;
  border: 2rpx solid #FF9833;
  border-radius: 4rpx;
  margin-right: 10rpx;
}
.select-btn image {
  width: 24rpx;
  height: 24rpx;
  display: none;
}
.select-btn.checked image {
  display: block;
}
.input-row {
  position: relative;
}
.input-row .title {
  width: 200rpx;
}
.input-row .des {
  position: absolute;
  right: 100rpx;
  top: 50%;
  -webkit-transform: translateY(-50%);
  transform: translateY(-50%);
  color: #BBB;
  font-size: 30rpx;
}
.input-row .idcard {
  height: 100rpx;
  position: absolute;
  right: 100rpx;
  top: 50%;
  -webkit-transform: translateY(-50%);
  transform: translateY(-50%);
}
.input-row .idcard2 {
  right: 120rpx;
}
.input-row .workcard {
  height: 100rpx;
  position: absolute;
  right: 100rpx;
  top: 50%;
  -webkit-transform: translateY(-50%);
  transform: translateY(-50%);
}
.reg-label {
  margin: 40rpx 0 0 0;
}
.btn-row {
  width: 690rpx;
  margin: 150rpx auto 50rpx;
}
.picker-item {
  width: 100%;
  text-align: right !important;
  font-size: 34rpx !important;
  color: #454545;
}
.picker-item .picker-btn {
  height: 116rpx;
  line-height: 116rpx;
  padding-right: 30rpx;
}